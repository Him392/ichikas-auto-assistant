    "version": "0.2.0",
    "configurations": [
        {
            "name": "main",
            "type": "debugpy",
            "request": "launch",
            "module": "iaa.main"
        },
        {
            "name": "main (debug)",
            "type": "debugpy",
            "request": "launch",
            "module": "iaa.main",
            "args": ["--debug"]
        },
        {
            "name": "current task",
            "type": "debugpy",
            "request": "launch",
            "module": "iaa.main",
            "args": ["--task", "${fileBasenameNoExtension}"]
        },
        {
            "name": "current task (debug)",
            "type": "debugpy",
            "request": "launch",
            "module": "iaa.main",
            "args": ["--task", "${fileBasenameNoExtension}", "--debug"]
        },
        {
            "name": "current task (justMyCode=false)",
            "type": "debugpy",
            "request": "launch",
            "module": "iaa.main",
            "args": ["--task", "${fileBasenameNoExtension}"],
            "justMyCode": false
        },
    ]
}